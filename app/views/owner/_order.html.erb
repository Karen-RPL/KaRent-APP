<tr id=<%= dom_id order %>>
  <td><i class="fab fa-angular fa-lg text-danger me-3"></i> <strong><%= order.name.titleize %></strong></td>

  <td>
    <div class="dropdown">
      <button type="button" class="btn p-0 dropdown-toggle hide-arrow" data-bs-toggle="dropdown">
        <i class="bx bx-dots-vertical-rounded"></i>
      </button>
      <div class="dropdown-menu">
        <%#= link_to "Edit this car", edit_car_path(order), :class=>"dropdown-item" %>
        <%= button_to "Destroy this order", order, method: :delete, :class=>"dropdown-item" %>
      </div>
    </div>
  </td>
</tr>